@Article{sdeogds,
  author       = "Christopher~W. Fraser",
  title        = "Syntax-Directed Editing of General Data Structures",
  journal      = sigplan # " (" # pot # "ACM SIGPLAN SIGOA Symposium on Text Manipulation)",
