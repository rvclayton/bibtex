@Article{wb3msanr,
  author       = "Panagiotis Demestichas and Louisa Papadopoulou and Vera
    Stavroulaki and Michael Theologou and Guillaume Vivier and Georges Martinez
    and Fabio Galliano",
  title        = "Wireless Beyond {3G}: Managing Services and Network Resources",
  journal      = ieeec,
  year         = 2002,
  volume       = 35,
  number       = 8,
  pages        = "80--82",
  month        = aug,
  keywords     = "wireless, federating, network management",
  abstract     = "Wireless beyond 3G is a new concept that envisions cellular,
    BFWA, and DVB systems cooperating in a composite radio infrastructure.  In
    this scheme, instead of rejecting users or degrading their quality of
    service levels, a network provider can direct them to an alternate radio
