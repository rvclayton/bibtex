@Article{dmscatldp,
  author       = "Tim Manzies and Jeremy Greenwald and Art Frank",
  title        = "Data Mining Static Code Attributes to Learn Defect Predictors",
  journal      = tse,
  year         = 2007,
  volume       = 33,
  number       = 1,
  pages        = "2--13",
  month        = jan,
  keywords     = "Data mining detect prediction, McCabe, Halstead, artifical
    intelligence, empirical, naive Bayes",
  abstract     = {The value of using static code attributes to learn defect 
    predictors has been widely debated.  Prior work has explored issues like
    the merits of "McCabes versus Halstead versus lines of code counts" for
    generating defect predictors. We show here that such debates are irrelevant
    since how the attributes are used to build predictors is much more
    important than which particular attributes are used.  Also, contrary to
    prior pessimism, we show that such defect predictors are demonstrably
    useful and, on the data studied here, yield predictors with a mean
    probability of detection of 71 percent and mean false alarms rates of 25
    percent.  These predictors would be useful for prioritizing a
    resource-bound exploration of code that has yet to be inspected.},
  location     = "QA 76.758.A35"
}

@Article{macilcacp,
