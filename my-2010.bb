@Article{patpoine,
  author       = "Philip~E. Agre",
  title        = "{P2P} and the Promise of {I}nternet Equality",
  journal      = cacm,
  year         = 2003,
  volume       = 46,
  number       = 2,
  pages        = "39--42",
