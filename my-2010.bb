@InProceedings{siwtimsi,
  author       = "Michael~T. Hunter and Russell~J. Clark and Frank~S. Park",
  title        = "Security Issues with the {IP} Multimedia Subsystem ({IMS})",
  booktitle    = "Workshop on Middleware for Next-generation Converged Networks
