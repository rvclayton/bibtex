@MastersThesis{dottuoars,
  author       = "John Whaley",
  title        = "Dynamic Optimization through the use of Automatic Runtime Specialization",
  school       = deecs # ", " # mit,
  year         = 1999,
  address      = cma,
  month        = may,
  abstract     = "Profile-driven optimizations and dynamic optimization through
		  specialization have taken optimizations to a new level.  By
