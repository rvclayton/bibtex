@TechReport{teotip,
  author       = "Jeremy Gibbons and Bruno~C. d.~S. Oliveira",
  title        = "The Essence of the {\sc Iterator} Pattern",
  institution  = "Oxford University Computing Laboratory",
  year         = 2006,
