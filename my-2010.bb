@InProceedings{iabpis,
  author       = "Jean-Michel Hufflen",
  title        = "Implementing a Bibliography Processor in {S}cheme",
  booktitle    = pot # "Sixth " # wsfp,
  year         = 2005,
  editor       = "J.~Michael Ashley and Michael Sperber",
  pages        = "77--87",
  address      = "Tallinn, Estonia",
  month        = "24 " # sep,
  keywords     = "mibibtex, document processing, MlBIBTEX, bibliography
    processor",
  abstract     = "We report an experience of implementing the MlBIBTEX
    bibliography processor, a re-implementation of BIBTEX with particular focus
    on multilingual features.  First we describe the behaviour of this software
    and explain why we chose Scheme to implement the first public version.
    Then we give the broad outlines of our implementation and show how we took
    as much advantage as possible of the main features of Scheme.  We also
    explain what we really missed and suggest some ways to improve these
    points.",
  location     = "http://www.cs.indiana.edu/pub/techreports/TR619.pdf"
}

@InProceedings{tmomam,
  author       = "Chongkai Zhu",
  title        = "The Marriage of {MrMathematica} and {MzScheme}",
  booktitle    = pot # "Sixth " # wsfp,
  year         = 2005,
  editor       = "J.~Michael Ashley and Michael Sperber",
  pages        = "89--92",
  address      = "Tallinn, Estonia",
  month        = "24 " # sep,
  keywords     = "computer algebra systems, mathematica",
  abstract     = "In this paper, I argue that the programming languages
