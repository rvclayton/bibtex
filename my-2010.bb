@Article{etimg,
  author       = "Silvana Trimi and Hong Sheng",
  title        = "Emerging trends in {M}-government",
  journal      = cacm,
  year         = 2008,
  volume       = 51,
  number       = 5,
  pages        = "53--58",
  month        = may,
