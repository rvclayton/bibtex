@Article{eaosotoa,
  author       = "Michael Franz",
  title        = "Emulating an Operating System on Top of Another",
  journal      = spe,
  year         = 1993,
  volume       = 23,
  number       = 6,
  pages        = "677--692",
  month        = "jun",
  keywords     = "software engineering, operating systems, software
