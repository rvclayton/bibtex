@Article{goeptdcoxscg,
  author       = "E. Perkins and M Matsa and M.G. Kostoulas and A. Heifets and
		  N Mendelsohn",
  title        = "Generation of efficient parsers through direct compilation of {XML} {S}chema grammars",
  journal      = ibmsj,
  year         = 2006,
  volume       = 25,
  number       = 2,
  pages        = "225--244",
  month        = jan,
  keywords     = "xml schemas, dynamic typing, xml namespaces, content models,
		  compilation and validation",
  abstract     = "With the widespread adoption of SOAP and Web services,
		  XML-based processing, and parsing of XML documents in
		  particular, is becoming a performance-critical aspect of
		  business computing.  In such scenarios, XML is often
		  constrained by an XML Schema grammar, which can be used
		  during parsing to improve performance. Although traditional
		  grammar-based parser generation techniques could be applied
		  to the XML Schema grammar, the expressiveness of XML.  Schema
		  does not lend itself well to the generic intermediate
		  representations associated with these approaches.  In this
		  paper we present a method for generating efficient parsers by
		  using the schema component model itself as the representation
		  of the grammar.  We show that the model supports the full
		  expressive power of the XML Schema, and we present results
		  demonstrating significant performance improvements over
		  existing parsers.",
  location     = "http://www.research.ibm.com/journal/sj/452/perkins.html"
}

