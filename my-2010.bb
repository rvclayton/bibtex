}

@Article{martd,
  author       = "John~A. Stankovic and Sang Hyuk Son and Jorgen Hansson",
  title        = "Misconceptions About Real-Time Databases",
  journal      = ieeec,
  year         = 1999,
  volume       = 32,
  number       = 6,
  pages        = "29--36",
  month        = jun,
  keywords     = "real-time databases, consistency, distributed databases",
  abstract     = "Increasingly, computer systems--even general-purpose 
    systems--are requiring real-time support, so it's not surprising to hear
    more about real-time databases.Unfortunately, there are many misconceptions
    about the real-time aspects of databases.  Mainstream database users who
    don't have any experience with real-time systems mistakenly think they are
    synonymous with speed.  Others argue that conventional database technology
    can be used in real-time applications.To help bring order to the current
    state of confusion, the authors review nine common misconceptions about
    real-time databases.  They also describe key research areas that need to be
    addressed to encourage increased activity in this challenging area.", 
  location     = "http://dx.doi.org/10.1109/2.769440"
}

@Article{caccfumr,
  author       = "Jan Blom and Jan Chipchase and Naakko Lehikoinen",
  title        = "Contextual and Cultural Challenges for User Mobility Research",
  journal      = cacm,
  year         = 2005,
  volume       = 48,
  number       = 7,
  pages        = "37--41",
  month        = jul,
  keywords     = "cultures, mobility research, field research",
  abstract     = "Personal, mobile synchronous and asynchronous communication 
    has proven to be very desirable for all types of users, with estimates of
    more than a half-billion mobile phones sold each year [2].  Nokia Research
    Center's User Experience Group, working with other user practitioners in
    Nokia, seeks to understand why people do the things they do with their
    mobile communication devices and proposes solutions that best address their
    wants and needs.  In accordance with the principles of user-centric product
    concept design [4], the solutions are designed to inform and inspire the
    product creation process within Nokia.  Given that the group works three to
    eight years ahead of what appears on the market, confidentiality concerns
    restrict disclosure of many of these concepts and the findings on which
    these concepts are based.  Instead, in this article we share some of the
    essential lessons learned from our projects centering on the early stages
    of product concept development.  Two areas have proved to be particularly
    challenging in this respect: coping with multiple contexts and multiple
    cultures in the study of mobility.",
  OPTlocation  = ""
}

