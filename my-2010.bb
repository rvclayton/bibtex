@Article{dcgflaasotfaacwatn,
  author       = "Fernando C.N. Pereira and David H.D. Warren",
  title        = "Definite Clause Grammars for Language Analysis---A Survey of the Formalism and a Comparison with Augmented Transition Networks",
  journal      = "Artificial Intelligence",
  year         = 1980,
  volume       = 13,
  number       = 3,
  pages        = "231--278",
  abstract     = {A clear and powerful formalism for describing languages, both
		  natural and artificial, follows from a method for expression
		  grammars in logic due to Colmerauer and Kowalski.  This
		  formalism, which is a natural extension of context-free
		  grammars, we call "definite clause grammars" (DCG).  A DCG
		  provides not only a description of a language, but also an
		  effective means for analyzing strings of that language, since
		  the DCG, as it stands, is an executable program of the
		  programming language Prolog.  Using a standard Prolog
		  compiler, the DCG can be compiled into efficient code, making
		  it feasible to implement practical language analyzers
		  directly as DCGs.  The paper compares DCGs with the
		  successful and widely used augmented transition network (ATN)
		  formalism, and indicates how ATNs can be translated into
		  DCGs.  It is argued that DCGs can be at least as efficient as
		  ATNs, whilst the DCG formalism is clearer, more concise and
		  in practice more powerful.},
  keywords     = "definite clause grammars, prolog, augmented transition
		  networks, declarative semantics, logic programming",
  location     = ""
}

