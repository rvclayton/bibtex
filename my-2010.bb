@InProceedings{hsftrs,
  author       = "James~C. Hoe and Arvind",
  title        = "Hardware Synthesis from Term Rewriting Systems",
  booktitle    = pot # "IFIP TC10/WG10.5 Tenth International Conference on Very
		  Large Scale Integration: Systems on a Chip",
  year         = 1999,
  pages        = "595--619",
  publisher    = "Kluwer",
  address      = "Lisbon, Portugal",
  month        = dec,
  keywords     = "term rewriting systems, high level description, high level
		  synthesis, trac", 
  abstract     = "Term Rewriting System (TRS) is a good formalism for
		  describing concurrent sys-tems that embody asynchronous and
		  nondeterministic behavior in their specifications.
		  Elsewhere, we have used TRS's to describe speculative
		  micro-architectures and complex cache-coherence protocols,
		  and proven the correctness of these systems.  In this paper,
		  we describe the compilation of TRS's into a subset of Verilog
		  that can be simulated and synthesized using commercial tools.
		  TRAC, Term Rewriting Architecture Compiler, enables a new
		  hardware development frame-work that can match the ease of
		  today's software programming environment.  TRAC reduces the
		  time and effort in developing and debugging hardware.  For
		  several examples, we compare TRAC-generated RTL's with
		  hand-coded RTL's after they are both compiled for Field
		  Programmable Gate Arrays by Xilinx tools.  The circuits
		  generated from TRS are competitive with those described using
		  Verilog RTL, especially for larger designs.",
  location     = "http://csg.lcs.mit.edu/pubs/memos/Memo-421a/memo-421a.pdf"
}

@InProceedings{cmtth,
  author       = "Tim Harris and Simon Marlow and Simon Peyton Jones and
		  Muriece Herlihy",
  title        = "Composable Memory Transactions",
  booktitle    = pot # "Tenth ACM SIGPLAN Symposium on Principles and Practice
		  of Parallel Programming",
  year         = 2005,
  pages        = "48--60",
  publisher    = acmp,
  address      = chil,
  month        = "15--17 " # jun,
  keywords     = "non-blocking algorithms, locks, transactions, haskell, type
		  systems, composible transactions, atomicity",
  abstract     = "Writing concurrent programs is notoriously difficult, and is
		  of increasing practical importance.  A particular source of
		  concern is that even correctly-implemented concurrency
		  abstractions cannot be composed together to form larger
		  abstractions.  In this paper we present a new concurrency
		  model, based on transactional memory, that offers far richer
		  composition.  All the usual benefits of transactional memory
		  are present (e.g.  freedom from deadlock), but in addition we
		  describe new modular forms of blocking and choice that have
		  been inaccessible in earlier work.",
  location     = "http://research.microsoft.com/~simonpj/papers/stm/stm.pdf"
}

@InProceedings{etdoainswaaca,
  author       = "Sarfraz Khurshid and Daniel Jackson",
  title        = "Exploring the Deisgn of an Intentional Naming Scheme with an Automatic Constraint Analyzer",
  booktitle    = pot # "15th " # icase,
  year         = 2000,
  pages        = "13--22",
  publisher    = "IEEE Computer Society",
  address      = "Grenoble, France",
  month        = "11--15 " # sep,
  keywords     = "intentional naming, alloy, modeling, model analysis",
  abstract     = "Lightweight formal modeling and automatic analysis were used
		  to explore the design of the Intentional Naming System (INS),
		  a new scheme for resource discovery in a dynamic networked
		  environment.  We constructed a model of INS in Alloy, a
		  lightweight relational notation, and analyzed it with the
		  Alloy Constraint Analyzer, a fully automatic simulation and
