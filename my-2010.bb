@InProceedings{aesocapppio,
  author       = "Miryung Kim and Lawrence Bergman and Tessa~A. Lau and David Notkin",
  title        = "An Ethnographic Study of Copy and Paste Programming Practices in {OOPL}",
  booktitle    = pot # "2004 International Symposium on Empirical Software Engineering",
  year         = 2004,
  pages        = "83--92",
  publisher    = ieeep,
  address      = "Redondo Beach, " # CA,
  month        = "19--20 " # aug,
  abstract     = "Although programmers frequently copy and paste code when they
		  develop software, implications of common copy and paste (C&P)
		  usage patterns have not been studied previously. We have
		  conducted an ethnographic study in order to understand
		  programmers' C&P programming practices and discover
		  opportunities to assist common C&P usage patterns. We
		  observed programmers using an instrumented Eclipse IDE and
		  then analyzed why and how they use C&P operations. Based on
		  our analysis, we constructed a taxonomy of C&P usage
		  patterns. This paper presents our taxonomy of C&P usage
		  patterns and discusses our insights with examples drawn from
		  our observations. From our insights, we propose a set of
		  tools that both can reduce software maintenance problems
		  incurred by C&P and can better support the intents of
		  commonly used C&P scenarios.",
  keywords     = "cut and paste programming, software design, ethnographic
		  studies",
  location     = "http://www.cs.washington.edu/homes/miryung/Publication/MiryungKim_Quals.pdf"
}

